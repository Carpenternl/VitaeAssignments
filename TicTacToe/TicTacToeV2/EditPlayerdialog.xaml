<Window x:Class="TicTacToeV2.EditPlayerdialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TicTacToeV2"
        mc:Ignorable="d"
        Title="EditPlayerdialog" Height="150" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="10"/>
            <RowDefinition Height="31*"/>
            <RowDefinition Height="47*"/>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="10"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="10"/>
            <ColumnDefinition Width="67*"/>
            <ColumnDefinition Width="70*"/>
            <ColumnDefinition Width="10"/>
        </Grid.ColumnDefinitions>
        <Button Grid.Row="3" Grid.Column="1" Content="CANCEL" IsCancel="True" Margin="2.4,2,2,2.2" />
        <Button Grid.Row="3" Grid.Column="2" Content="OK" IsDefault="True" Margin="2,2,2,2.2"/>
    </Grid>
</Window>
